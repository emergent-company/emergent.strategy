{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "name": "admin",
    "projectType": "application",
    "sourceRoot": "apps/admin/src",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm --prefix apps/admin run build",
                "forwardAllArgs": true
            }
        },
        "dev": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm --prefix apps/admin run dev",
                "forwardAllArgs": true
            }
        },
        "e2e": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm --prefix apps/admin run e2e",
                "forwardAllArgs": true
            }
        },
        "e2e-setup": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm --prefix apps/admin run e2e:setup",
                "forwardAllArgs": true
            }
        },
        "e2e-ui": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm --prefix apps/admin run e2e:ui",
                "forwardAllArgs": true
            }
        },
        "lint": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm --prefix apps/admin run lint",
                "forwardAllArgs": true
            }
        },
        "setup": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npx tsx tools/workspace-cli/src/cli.ts setup --service=admin",
                "forwardAllArgs": true
            }
        },
        "storybook": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm --prefix apps/admin run storybook",
                "forwardAllArgs": true
            }
        },
        "storybook-build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm --prefix apps/admin run build-storybook",
                "forwardAllArgs": true
            }
        },
        "start": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npx tsx tools/workspace-cli/src/cli.ts start --service=admin",
                "forwardAllArgs": true
            }
        },
        "restart": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npx tsx tools/workspace-cli/src/cli.ts restart --service=admin",
                "forwardAllArgs": true
            }
        },
        "stop": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npx tsx tools/workspace-cli/src/cli.ts stop --service=admin",
                "forwardAllArgs": true
            }
        },
        "test-coverage": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm --prefix apps/admin run test:coverage",
                "forwardAllArgs": true
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm --prefix apps/admin run test -- --run",
                "forwardAllArgs": true
            }
        }
    },
    "tags": [
        "scope:admin"
    ]
}