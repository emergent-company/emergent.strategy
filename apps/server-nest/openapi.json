              "application/json": {
                "schema": {
                  "example": {
                    "error": {
                      "code": "validation-failed",
                      "message": "Validation failed",
                      "details": {
                        "q": [
                          "must be longer than or equal to 1 characters"
                        ]
                      }
                    }
                  }
                }
              }
            }
          },
