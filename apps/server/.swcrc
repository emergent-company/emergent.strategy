{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "exclude": [
    "src/migrations-archive/.*",
    ".*\\.spec\\.ts$",
    ".*/__tests__/.*"
  ],
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "target": "es2022",
    "keepClassNames": true,
    "baseUrl": "./"
  },
  "module": {
    "type": "commonjs"
  }
}
