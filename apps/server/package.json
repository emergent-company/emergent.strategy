    "typeorm": "^0.3.27",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/testing": "^10.3.0",
    "@swc/cli": "^0.7.9",
    "@swc/core": "~1.5.29",
    "@types/express": "^5.0.3",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^27.0.0",
    "@types/mjml": "^4.7.4",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.17.2",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^2.0.16",
    "@types/turndown": "^5.0.6",
    "@vitest/coverage-v8": "^3.2.4",
    "multer": "^2.0.2",
    "rimraf": "^5.0.5",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  }
}
