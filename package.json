{
  "name": "spec-server",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/admin",
    "apps/server",
    "tools/workspace-cli"
  ],
  "type": "module",
  "scripts": {
    "prepare": "husky || true",
    "nx": "nx",
    "clean": "npm --prefix apps/server run clean || true && rimraf apps/admin/dist || true",
    "dev:server": "nx run server:start-dev",
    "dev:admin": "nx run admin:dev",
    "dev:storybook": "nx run admin:serve:storybook -- --port=6006",
    "workspace:start": "node tools/workspace-cli/dist/cli.js start --workspace",
    "workspace:start-all": "node tools/workspace-cli/dist/cli.js start --all",
    "workspace:stop": "node tools/workspace-cli/dist/cli.js stop --workspace",
    "workspace:restart": "node tools/workspace-cli/dist/cli.js restart --workspace",
    "workspace:status": "node tools/workspace-cli/dist/cli.js status --workspace --dependencies",
    "workspace:deps:start": "node tools/workspace-cli/dist/cli.js start --deps-only",
    "workspace:deps:stop": "node tools/workspace-cli/dist/cli.js stop --deps-only",
    "workspace:deps:restart": "node tools/workspace-cli/dist/cli.js restart --deps-only",
    "start": "node apps/server/dist/main.js",
    "build:server": "nx run server:build",
    "build:admin": "nx run admin:build",
    "build": "npm run clean && npm run build:server && npm run build:admin",
    "test:e2e:server": "nx run server:test-e2e",
    "test:scenarios:server": "nx run server:test-scenarios",
    "spec:diff": "nx run repo-scripts:spec-diff",
    "db:init": "tsx scripts/init-db.ts",
    "db:reset": "tsx scripts/reset-db.ts",
    "db:full-reset": "tsx scripts/full-reset-db.ts",
    "db:migrate": "cd apps/server && NODE_OPTIONS=\"--import tsx\" npx typeorm migration:run -d src/typeorm.config.ts",
    "db:migrate:generate": "cd apps/server && NODE_OPTIONS=\"--import tsx\" npx typeorm migration:generate -d src/typeorm.config.ts",
    "db:migrate:revert": "cd apps/server && NODE_OPTIONS=\"--import tsx\" npx typeorm migration:revert -d src/typeorm.config.ts",
    "db:validate": "tsx scripts/validate-schema.ts",
    "db:fix": "tsx scripts/validate-schema.ts --fix",
    "db:diff": "tsx scripts/validate-schema.ts --diff --verbose",
    "db:docs:generate": "bash -c 'source .env 2>/dev/null || true; dbdocs db2dbml postgres \"postgresql://${POSTGRES_USER:-spec}:${POSTGRES_PASSWORD:-spec}@${POSTGRES_HOST:-localhost}:${POSTGRES_PORT:-5437}/${POSTGRES_DB:-spec}\" > docs/database/schema.dbml'",
    "db:docs:validate": "dbdocs validate docs/database/schema.dbml",
    "db:docs:local": "npm run db:docs:generate && npm run db:docs:validate",
    "db:e2e:migrate": "dotenv -e .env.e2e -- npm run db:migrate",
    "db:e2e:up": "docker compose -f docker/e2e/docker-compose.yml up -d",
    "db:e2e:down": "docker compose -f docker/e2e/docker-compose.yml down",
    "db:e2e:restart": "npm run db:e2e:down && npm run db:e2e:up",
    "db:e2e:logs": "docker compose -f docker/e2e/docker-compose.yml logs -f",
    "chrome:debug": "./scripts/start-chrome-debug.sh http://localhost:5176",
    "chrome:debug:status": "./scripts/start-chrome-debug.sh --status",
    "secrets:dump": "tsx scripts/dump-infisical-secrets.ts",
    "secrets:dump:dry-run": "tsx scripts/dump-infisical-secrets.ts -- --dry-run",
    "migrate-secrets": "tsx scripts/migrate-secrets-to-infisical.ts",
    "migrate-secrets:dry-run": "tsx scripts/migrate-secrets-to-infisical.ts -- --dry-run",
    "audit-infisical-duplicates": "tsx scripts/audit-infisical-duplicates.ts",
    "db:e2e:clean": "docker compose -f docker/e2e/docker-compose.yml down -v",
    "db:e2e:wait": "until docker compose -f docker/e2e/docker-compose.yml exec -T db pg_isready -U spec -d spec_e2e; do sleep 1; done",
    "test:e2e": "npm run db:e2e:restart && npm run db:e2e:wait && npm run db:e2e:migrate && nx run server:test-e2e && npm run db:e2e:down",
    "migrate:embedding-dimension": "tsx scripts/migrate-embedding-dimension.ts",
    "test:smoke": "nx run repo-scripts:test-smoke",
    "mcp:db": "node .vscode/mcp.js",
    "check:stories": "node scripts/check-stories.mjs",
    "ref:init": "git submodule update --init --recursive",
    "ref:pull": "git -C reference/nexus pull origin master",
    "e2e:admin:real": ". ./.env && E2E_REAL_LOGIN=1 nx run admin:e2e",
    "e2e:admin:ui": ". ./.env && E2E_REAL_LOGIN=1 nx run admin:e2e-ui",
    "e2e:admin:setup": ". ./.env && E2E_REAL_LOGIN=1 nx run admin:e2e-setup",
    "bench:graph:relationships": "npm --prefix apps/server run bench:graph:relationships",
    "bench:graph:objects": "npm --prefix apps/server run bench:graph:objects",
    "bench:graph:traverse": "npm --prefix apps/server run bench:graph:traverse",
    "test:coverage:admin": "nx run admin:test-coverage",
    "test:coverage:server": "nx run server:test-coverage",
    "test:coverage:server:e2e": "nx run server:test-coverage-e2e",
    "test:coverage:server:all": "nx run server:test-coverage && nx run server:test-coverage-e2e",
    "test:coverage:all": "nx run repo-scripts:test-coverage-all",
    "log:pretty": "node scripts/pretty-print-log.js",
    "log:clickup": "node scripts/pretty-print-log.js logs/clickup-import/*.jsonl",
    "sync:instructions": "node scripts/sync-agent-instructions.mjs",
    "seed:bible-template": "tsx scripts/seed-bible-template-pack.ts",
    "seed:bible-template-enhanced": "tsx scripts/seed-bible-template-pack-enhanced.ts",
    "seed:bible": "tsx scripts/seed-bible-documents.ts",
    "seed:togaf": "tsx scripts/seed-togaf-template.ts",
    "seed:epf": "tsx scripts/seed-emergent-framework.ts",
    "seed:meeting": "tsx scripts/seed-meeting-pack.ts",
    "seed:extraction-demo": "tsx scripts/seed-extraction-demo.ts",
    "seed:templates": "tsx scripts/seed-togaf-template.ts && tsx scripts/seed-emergent-framework.ts && tsx scripts/seed-meeting-pack.ts && tsx scripts/seed-bible-template-pack.ts",
    "migrate:bible-v1-to-v2": "tsx scripts/migrate-bible-v1-to-v2.ts",
    "migrate:embedded-relationships": "tsx scripts/migrate-embedded-relationships.ts",
    "migrate:embedded-relationships:dry-run": "tsx scripts/migrate-embedded-relationships.ts -- --dry-run",
    "reset-embedding-jobs": "tsx scripts/reset-embedding-jobs.ts",
    "reset-embedding-jobs:execute": "tsx scripts/reset-embedding-jobs.ts --execute",
    "backfill-chunk-embeddings": "tsx scripts/backfill-chunk-embeddings.ts",
    "backfill-chunk-embeddings:execute": "tsx scripts/backfill-chunk-embeddings.ts --execute",
    "verify-env": "node scripts/verify-env.mjs",
    "lint:duplicates": "jscpd apps/ --config .jscpd.json",
    "lint:duplicates:full": "jscpd apps/ --min-lines 10 --min-tokens 50 --reporters console,json --output ./reports/jscpd --ignore '**/node_modules/**'"
  },
  "dependencies": {
    "@ai-sdk/langchain": "^1.0.104",
    "@ai-sdk/react": "^2.0.97",
    "@api/clickup": "file:.api/apis/clickup",
    "@google/genai": "^1.32.0",
    "@langchain/core": "^1.1.0",
    "@langchain/google-genai": "^2.0.0",
    "@langchain/google-vertexai": "^1.0.4",
    "@langchain/langgraph": "^1.0.2",
    "@langchain/langgraph-checkpoint-postgres": "^1.0.0",
    "@langchain/textsplitters": "^1.0.1",
    "@langfuse/langchain": "^4.4.9",
    "@modelcontextprotocol/sdk": "^1.1.7",
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/swagger": "^7.4.2",
    "@types/uuid": "^11.0.0",
    "adm-zip": "^0.5.16",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "framer-motion": "^12.23.24",
    "google-auth-library": "^10.5.0",
    "html-to-text": "^9.0.5",
    "jose": "^5.9.6",
    "langchain": "^1.0.0",
    "langextract": "^1.2.0",
    "langfuse-node": "^3.38.6",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "p-limit": "^5.0.0",
    "passport": "^0.7.0",
    "passport-zitadel": "^1.2.3",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@infisical/sdk": "^4.0.6",
    "@nx/devkit": "^21.6.5",
    "@nx/js": "21.6.5",
    "@nx/storybook": "^21.6.5",
    "@nx/web": "21.6.5",
    "@nx/workspace": "^21.6.5",
    "@storybook/react": "^10.1.3",
    "@storybook/react-vite": "9.1.13",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tailwindcss/vite": "^4.1.17",
    "@types/adm-zip": "^0.5.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/html-to-text": "^9.0.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.17.2",
    "@types/pg": "^8.15.5",
    "chalk": "^5.6.2",
    "concurrently": "^9.1.2",
    "dotenv-cli": "^10.0.0",
    "execa": "^9.6.0",
    "husky": "^9.1.7",
    "nx": "^21.6.5",
    "prettier": "^2.6.2",
    "rimraf": "^5.0.5",
    "simplebar-core": "^1.3.2",
    "storybook": "9.1.13",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "typescript-language-server": "^5.1.3"
  },
  "overrides": {
    "@modelcontextprotocol/sdk": {
      "express": "4.21.2"
    }
  },
  "pnpm": {
    "overrides": {
      "@opentelemetry/sdk-trace-base": "2.0.0",
      "@opentelemetry/resources": "2.0.0",
      "@opentelemetry/sdk-metrics": "2.0.0",
      "@opentelemetry/core": "2.0.0",
      "@opentelemetry/api-logs": "0.208.0",
      "@opentelemetry/sdk-logs": "0.208.0"
    }
  }
}
