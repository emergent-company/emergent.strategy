{
  "name": "logs-mcp",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/logs-mcp/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p tsconfig.json",
        "cwd": "tools/logs-mcp"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p tsconfig.json --noEmit",
        "cwd": "tools/logs-mcp"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx src/index.ts",
        "cwd": "tools/logs-mcp"
      }
    }
  },
  "tags": ["scope:tools", "type:mcp"]
}
